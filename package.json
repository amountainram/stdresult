{
  "name": "stdresult",
  "version": "0.1.0",
  "type": "module",
  "author": "Umberto Toniolo <amountainram@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/amountainram/stdresult"
  },
  "description": "Rust-inspired, type-safe Result and async error handling utilities for TypeScript, with ergonomic wrappers for native JS APIs.",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "browser": "./dist/index.min.js",
      "deno": "./src/index.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build:types": "tsc --project ./scripts/tsconfig.json",
    "build:lib": "tsx ./scripts/build.ts",
    "build": "run-p build:types build:lib",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.9",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "zod": "^4.1.9"
  }
}
